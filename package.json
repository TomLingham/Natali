    "@babel/preset-env": "^7.1.0",
    "@babel/preset-flow": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "chalk": "^2.4.1",
    "detect-indent": "^5.0.0",
    "globby": "^8.0.1",
    "js-yaml": "^3.12.0",
    "log-driver": "^1.2.7",
    "mustache": "^3.0.0",
    "request": "^2.88.0",
    "yargs": "^12.0.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "eslint": "^5.6.1",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-prettier": "^3.0.0",
    "flow-bin": "^0.82.0",
    "flow-typed": "^2.5.1",
    "jest": "^23.6.0",
    "prettier": "^1.14.3"
  }
